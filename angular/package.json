{
  "name": "tanam-angular",
  "version": "0.0.0",
  "scripts": {
    "start:admin": "ng serve admin",
    "build:admin": "ng build admin --deployUrl=/ --aot --outputHashing=all --optimization=true",
    "build:admin:prod": "ng build admin --prod=true --deployUrl=/ --aot --outputHashing=all --optimization=true",
    "test:admin": "ng test admin",
    "lint:admin": "ng lint admin",
    "e2e:admin": "ng e2e admin",
    "build:core": "ng build tanam-core",
    "start:dynamic": "ng serve dynamic-render --port 4444",
    "build:dynamic": "ng build dynamic-render",
    "build:dynamic:prod": "ng build dynamic-render --deployUrl=/ --outputHashing=all",
    "test:dynamic": "ng test dynamic-render",
    "lint:dynamic": "ng lint dynamic-render",
    "e2e:dynamic": "ng e2e dynamic-render",
    "webpack:dynamic": "webpack --config webpack.server.config.js --progress --colors",
    "serve:dynamic:ssr": "node dist/dynamic/server",
    "build:dynamic:ssr": "npm run build:dynamic:bundle && npm run webpack:dynamic",
    "build:dynamic:bundle": "npm run build:dynamic:prod && ng run dynamic-render:server:production",
    "build:all": "npm run build:core && npm run build:admin:prod && npm run build:dynamic:ssr"
  },
  "engines": {
    "node": "8"
  },
  "dependencies": {
    "@angular/animations": "~7.2.1",
    "@angular/cdk": "^7.2.2",
    "@angular/common": "~7.2.1",
    "@angular/compiler": "^7.2.1",
    "@angular/core": "~7.2.1",
    "@angular/fire": "^5.1.1",
    "@angular/forms": "~7.2.1",
    "@angular/http": "~7.2.1",
    "@angular/material": "~7.2.1",
    "@angular/platform-browser": "~7.2.1",
    "@angular/platform-browser-dynamic": "~7.2.1",
    "@angular/platform-server": "~7.2.1",
    "@angular/router": "~7.2.1",
    "@ckeditor/ckeditor5-angular": "^1.0.1",
    "@ckeditor/ckeditor5-build-classic": "^11.2.0",
    "@nguniversal/express-engine": "^7.1.0",
    "@nguniversal/module-map-ngfactory-loader": "0.0.0",
    "core-js": "^2.5.4",
    "express": "^4.15.2",
    "firebase": "^5.8.0",
    "hammerjs": "^2.0.8",
    "rxjs": "~6.3.3",
    "ws": "^6.1.3",
    "xhr2": "^0.1.4",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.11.0",
    "@angular-devkit/build-ng-packagr": "~0.11.0",
    "@angular/cli": "~7.1.4",
    "@angular/compiler-cli": "^7.2.1",
    "@angular/language-service": "~7.1.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "bufferutil": "^4.0.1",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^4.2.0",
    "protractor": "~5.4.0",
    "ts-loader": "^5.2.0",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.29.0",
    "tslib": "^1.9.3",
    "tslint": "~5.11.0",
    "typescript": "~3.1.6",
    "utf-8-validate": "^5.0.2",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.1.0"
  }
}
